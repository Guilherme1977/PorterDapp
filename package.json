  "scripts": {
    "start": "react-app-rewired start",
    "prebuild": "yarn format",
    "build": "cross-env REACT_APP_GIT_COMMIT_HASH=$(git show -s --format=%H) yarn typechain:all && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "format:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format:prettier": "prettier \"src/**/*.{js,jsx,ts,tsx}\" --write",
    "format": "yarn format:eslint && yarn format:prettier",
    "eject": "react-app-rewired eject",
    "prepare": "husky install",
    "postinstall": "yarn typechain:all",
    "typechain:abi": "yarn typechain --target=ethers-v5 ./src/constants/abis/**/*.json --out-dir=./gen/types",
    "typechain:gql": "graphql-codegen --require dotenv/config --config .graphqlrc",
    "typechain:all": "yarn typechain:abi && yarn typechain:gql"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
